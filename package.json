{
  "name": "@ssv/au-core",
  "version": "0.3.7",
  "description": "Core components, utilities and services for Aurelia by Sketch7",
  "keywords": [
    "sketch7",
    "aurelia",
    "core",
    "utils",
    "logger",
    "router"
  ],
  "scripts": {
    "start": "gulp watch",
    "test": "jest",
    "tdd": "jest --watch",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "test-devt": "devtool ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "lint": "gulp lint",
    "build": "gulp build",
    "bundle": "gulp bundle:ts",
    "ssv-publish": "gulp ssv-publish"
  },
  "author": "Stephen Lautier <stephen.lautier@outlook.com>, Clayton Lautier <clayton.lautier@outlook.com>",
  "license": "MIT",
  "main": "dist/bundles/ssv-au-core.umd.js",
  "module": "dist/es2015/index.js",
  "typings": "dist/es2015/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/sketch7/ssv-au-core"
  },
  "devDependencies": {
    "@ssv/tools": "^0.6.11",
    "@types/jest": "^21.1.2",
    "@types/lodash": "^4.14.77",
    "aurelia-polyfills": "^1.2.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.8.0",
    "gulp-conventional-changelog": "^1.1.6",
    "gulp-git": "^2.4.2",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-util": "^3.0.8",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.2.0",
    "systemjs": "^0.20.19",
    "ts-jest": "^21.1.0",
    "typescript": "^2.5.3",
    "yargs": "^9.0.1"
  },
  "dependencies": {
    "@ssv/core": "^0.3.7",
    "aurelia-dependency-injection": "^1.3.2",
    "aurelia-event-aggregator": "^1.0.1",
    "aurelia-framework": "^1.1.5",
    "aurelia-logging": "^1.3.1",
    "aurelia-route-recognizer": "^1.1.1",
    "aurelia-templating": "^1.5.0",
    "lodash": "^4.17.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "setupTestFrameworkScriptFile": "<rootDir>/test/test-setup.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
