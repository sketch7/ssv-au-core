{
	"compilerOptions": {
		"module": "amd",
		"target": "es5",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"declaration": true,
		"noImplicitReturns": true,
		"noImplicitAny": true,
		"noFallthroughCasesInSwitch": true,
		"noEmitOnError": true
	},
	"exclude": [
		"node_modules",
		"wwwroot",
		"dist"
	]
}